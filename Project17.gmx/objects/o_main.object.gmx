            <string>//INIT
//STATE
enum player_states {
     idle, 
     move,
     jump,  
     hit,
     fall,
     doublejump,
     attack
}

//movement
grav = 0.5;
moveX = 0;
moveY = 0;
nor_spd = 3;
run_spd = 5;
count = 0;

// Init state
state = player_states.idle

globalvar left {
    left = false
}
score = 0
damage = false
x = o_start_checkpoint.x
y = o_start_checkpoint.y
